If o = "" Then MsgBox "Could not find Google Chrome or packaged GomegaBrowser.exe." & vbCrLf & "Please install Chrome or adjust the script paths.", vbExclamation, "Launcher Error" : window.close

' launch with UA if chrome
If LCase(Right(o, Len("chrome.exe"))) = "chrome.exe" Then
  p = "--user-agent=" & """" & j & """"
  q = """" & o & """ " & p & " " & """" & g & """ " & """" & h & """ " & """" & i & """"
  k.Run q,1,False
Else
